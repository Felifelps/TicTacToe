#:kivy 1.9.0
#:import grid grid

<Tile>:
    canvas:
        Line:
            rectangle: self.x, self.y, self.width, self.height
            width: 2
    size: 100, 100
    font_size: 40
    text: " "

<Grid>:
    cols: 3
    pos_hint: {"center_x": .5, "center_y": .5}
    size_hint: None, None
    size: 300, 300


    